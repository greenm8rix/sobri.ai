{
  "packageId": "ai.Soberi.twa",
  "host": "Soberi-ai.netlify.app",
  "name": "Soberi.ai",
  "launcherName": "Soberi.ai",
  "display": "standalone",
  "themeColor": "#4f46e5",
  "navigationColor": "#000000",
  "navigationColorDark": "#000000",
  "navigationDividerColor": "#000000",
  "navigationDividerColorDark": "#000000",
  "backgroundColor": "#f9fafb",
  "enableNotifications": true,
  "shortcuts": [
    {
      "name": "Chat with Soberi",
      "shortName": "Chat",
      "url": "/?tab=chat",
      "icon": "/shortcuts/chat.png"
    },
    {
      "name": "Daily Check-in",
      "shortName": "Check-in",
      "url": "/?tab=checkin",
      "icon": "/shortcuts/checkin.png"
    },
    {
      "name": "Today's Tasks",
      "shortName": "Tasks",
      "url": "/?tab=tasks",
      "icon": "/shortcuts/tasks.png"
    }
  ],
  "fallbackType": "customtabs",
  "features": {
    "locationDelegation": {
      "enabled": false
    },
    "playBilling": {
      "enabled": false
    }
  },
  "alphaDependencies": {
    "enabled": false
  },
  "enableSiteSettingsShortcut": true,
  "skipPwaValidation": false,
  "splashScreenFadeOutDuration": 300,
  "signingKey": {
    "path": "",
    "alias": ""
  },
  "appVersionName": "1.0.0",
  "appVersionCode": 1,
  "permissions": []
}